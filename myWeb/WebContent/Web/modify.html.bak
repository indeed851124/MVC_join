	<!--실제 내용 콘텐츠가 들어가는 곳-->
<article id="contents">
 <h1>Board</h1>
	<input type="button" value="list" class="btn"/> <!--게시판 목록으로 돌아가는 버튼-->
	<table id="board">
	<th id="img_bar">modify</th>
</table>
<div id="box">
	<div class="board">
			<form>
			<div><label class="label">name</label><input type="text" required autofocus size="25"></div>
			<div><label class="label">e-mail</label><input type="email" required size="25"></div>
			<div><label class="label">password</label><input type="password" required size="10">
					<input type="checkbox" >비밀글 작성시 체크해주세요.</div>
			<div><label class="label">title</label><input type="text" required size="50"></div>
			<div><label class="label">input text</label><textarea class="text_area"></textarea></div>
			<div><label class="label">file</label><input type="text" readonly size="70">
				<input type="button" value="찾기" class="btn"></div>

			<div class="board_btn">
							 <input type="reset" value="reset" class="btn">
						<input type="submit" value="save" class="btn">
				</div>
			</form>
		</div><!--.board 닫는태그-->
</div>

	
	
	<div class="clear"></div>
	
</article>


	
